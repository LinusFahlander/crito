@using Umbraco.Cms.Web.Common.PublishedModels;
@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage<ContentModels.HomePage>
@using ContentModels = Umbraco.Cms.Web.Common.PublishedModels;

<!-- Our-services -->
<section class="our-services">
    <img class="background-lines-right" src="~/images/Vector-lines/background-lines-right.svg" alt="">
    <div class="container">
        <div class="text">
            <h2>@Model.Value("ourServiceTitle")</h2>
            <h1>@Model.Value("ourServicesHeading")</h1>
        </div>
        <div class="info-cards">
            @Html.GetBlockListHtml(Model.OurServices)
        </div>
        <a class="btn-transparent" href="@Model.Value("ourServicesButtonURL")">@Model.Value("ourServicesButtonText")<i class="fa-regular fa-arrow-up-right"></i></a>
    </div>
</section>